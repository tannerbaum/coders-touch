{
	"name": "touchbar-symbols",
	"displayName": "touchbar-symbols",
	"description": "Puts essential programming symbols in touchbar",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.leftBracket",
		"onCommand:extension.rightBracket",
		"onCommand:extension.arrowFunction"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.leftBracket",
				"title": " { "
			},
			{
				"command": "extension.rightBracket",
				"title": " } "
			},
			{
				"command": "extension.arrowFunction",
				"title": " => "
			}
		],
		"menus": {
			"touchBar": [
				{
					"command": "extension.leftBracket",
					"group": "editing"
				},
				{
					"command": "extension.rightBracket",
					"group": "editing"
				},
				{
					"command": "extension.arrowFunction",
					"group": "editing"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.0-next.0"
	}
}
